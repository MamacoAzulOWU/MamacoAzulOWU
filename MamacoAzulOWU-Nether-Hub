--[
































































































































































































































































































































]--

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Nether Hub | Key System Deletada", HidePremium = true, SaveConfig = true, ConfigFolder = "OrionTest"})

_G.Key = "72662672882997271992272" -- You Put Your Key Here
_G.KeyInput = "string"

function MakeScriptHub()
loadstring(game:HttpGet("https://raw.githubusercontent.com/MamacoAzulOWU/MamacoAzulOWU/main/Nether-Hub-Load"))()

end


function CorrectKeyNotifications()
    OrionLib:MakeNotification({
        Name = "Codigo Certo",
        Content = "Aeee, Bem Vindo ao Script Nether Hub",
        Image = "rbxassetid://17590287332",
        Time = 5
    })
end

function WrongKeyNotifications()
    OrionLib:MakeNotification({
        Name = "Codigo Errado",
        Content = "Tente Novamente voce consegue",
        Image = "rbxassetid://17590287332",
        Time = 5
    })
end

local Tab = Window:MakeTab({
	Name = "Sala Principal",
	Icon = "rbxassetid://17590287332",
	PremiumOnly = false
})

Tab:AddTextbox({
	Name = "Checar CÃ³digo",
	Default = "Codigo?",
	TextDisappear = true,
	Callback = function(Value)
        _G.KeyInput = Value
	end	  
})

Tab:AddButton({
	Name = "Checar Codigo!",
	Callback = function()
        if _G.KeyInput == _G.Key then
        OrionLib:Destroy()
         MakeScriptHub()
         CorrectKeyNotifications()
     else
        WrongKeyNotifications()
        end
  	end    
})

Tab:AddButton({
	Name = "Copiar Codigo",
	Callback = function()
        setclipboard("72662672882997271992272")
  	end    
})
